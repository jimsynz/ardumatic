    <!-- Small elevation changes -->
    <model name="hill_1">
      <static>true</static>
      <pose>-3 -2 0.15 0 0.1 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>2.0 1.5 0.3</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
